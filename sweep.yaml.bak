program: train.py
method: bayes
metric:
  name: val_loss
  goal: minimize
parameters:
  epochs:
    distribution: constant
    value: 30
  learning_rate:
    distribution: q_uniform
    min: 0.001
    max: 0.01
    q: 0.001
  seq_length:
    distribution: q_uniform
    min: 50
    max: 150
    q: 10
  max_queue_size:
    distribution: constant
    value: 4096
  LSTM1_SIZE:
    distribution: categorical
    values:
      - 32
      - 64
      - 128
  LSTM2_SIZE:
    distribution: categorical
    values:
      - 16
      - 32
      - 64
  LSTM3_SIZE:
    distribution: categorical
    values:
      - 16
      - 32
      - 64
      - 128
  LSTM4_SIZE:
    distribution: categorical
    values:
      - 16
      - 32
      - 64
      - 128
  LSTM5_SIZE:
    distribution: categorical
    values:
      - 16
      - 32
      - 64
      - 128
  audio_path:
    distribution: constant
    value: data/train-noise-subtractive-250ms-1
early_terminate:
  type: hyperband
  max_iter: 27
  s: 2
